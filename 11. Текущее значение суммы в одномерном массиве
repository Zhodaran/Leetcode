func runningSum(nums []int) []int {
    n := len(nums)
    result := make([]int, n) // Создаем новый массив для хранения текущих значений суммы
    currentSum := 0           // Инициализируем переменную для текущей суммы

    // Проходим по всем элементам массива nums
    for i := 0; i < n; i++ {
        currentSum += nums[i] // Добавляем текущий элемент к текущей сумме
        result[i] = currentSum // Сохраняем текущую сумму в результирующий массив
    }

    return result // Возвращаем массив текущих значений суммы
}
